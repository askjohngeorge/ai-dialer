# Cal.com Configuration
CALCOM_API_KEY=cal_xxxx_xxxxxxxxxxxxxxxx
CALCOM_USER_ID=your-cal-username
CALCOM_EVENT_TYPE_ID=hvac-demo
CALCOM_EVENT_DURATION=30
CALCOM_WEBHOOK_SECRET=whsec_xxxxxxxxxxxxxxxxxxxx

# Database Configuration
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key           # Used for client-side authentication
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key       # Used for server-side operations (e.g., cron)

# VAPI Configuration
VAPI_API_KEY=vapi_xxxx_xxxxxxxxxxxxxxxx
VAPI_ASSISTANT_ID=asst_xxxx_xxxxxxxxxxxxxxxx
VAPI_PHONE_NUMBER_ID=phn_xxxx_xxxxxxxxxxxxxxxx

# VAPI Integration Webhook
VAPI_SECRET_KEY=your_generated_webhook_secret_here    # Secret for authenticating VAPI webhooks to our endpoints
HVAC_DIALLER_URL=https://your-domain.com             # Base URL for your HVAC dialler application

# Cron Configuration
CRON_SECRET=your-secret-here                         # Secret for authenticating cron job requests

# Email Configuration (Resend)
RESEND_API_KEY=re_xxxx_xxxxxxxxxxxxxxxx

# Node Environment
NODE_ENV=development
